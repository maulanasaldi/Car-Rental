	<queryString language="SQL">
		<![CDATA[SELECT
     pembayaran.`id_pembayaran` AS pembayaran_id_pembayaran,
     pembayaran.`id_pemesanan` AS pembayaran_id_pemesanan,
     pembayaran.`nama_pelanggan` AS pembayaran_nama_pelanggan,
     pembayaran.`no_telepon_pelanggan` AS pembayaran_no_telepon_pelanggan,
     pembayaran.`id_karyawan` AS pembayaran_id_karyawan,
     pembayaran.`tanggal_pembayaran` AS pembayaran_tanggal_pembayaran,
     pembayaran.`id_mobil` AS pembayaran_id_mobil,
     pembayaran.`harga_mobil` AS pembayaran_harga_mobil,
     pembayaran.`lama_sewa` AS pembayaran_lama_sewa,
     pembayaran.`dp` AS pembayaran_dp,
     pembayaran.`total` AS pembayaran_total,
     pembayaran.`supir` AS pembayaran_supir,
     pembayaran.`id_supir` AS pembayaran_id_supir,
     pembayaran.`nama_supir` AS pembayaran_nama_supir,
     pembayaran.`no_telepon_supir` AS pembayaran_no_telepon_supir,
     pembayaran.`tarif_supir` AS pembayaran_tarif_supir,
     pembayaran.`total_harga` AS pembayaran_total_harga,
     pembayaran.`uang` AS pembayaran_uang,
     pembayaran.`kembali` AS pembayaran_kembali
FROM
     `pembayaran` pembayaran]]>
	</queryString>
